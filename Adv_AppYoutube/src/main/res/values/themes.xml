<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.StartAndroid" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowContentOverlay">true</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/primary_color</item>
        <item name="colorPrimaryDark">@color/dark_primary_color</item>
        <item name="colorAccent">@color/accent_color</item>

        <item name="materialDrawerBackground">@color/drawer_background_color</item>
        <item name="materialDrawerPrimaryIcon">@color/text_and_icon_color</item>
        <item name="materialDrawerPrimaryText">@color/drawer_background_color</item>



</style>
</resources>